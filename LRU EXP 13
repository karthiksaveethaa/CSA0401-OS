#include <stdio.h>
int main(){
 int n,f,p[20],fr[10],i,j,k,min=0,count=0,time[10]={0},t=0;
 printf("Pages: "); scanf("%d",&n);
 printf("Seq: "); for(i=0;i<n;i++) scanf("%d",&p[i]);
 printf("Frames: "); scanf("%d",&f);
 for(i=0;i<f;i++) fr[i]=-1;
 for(i=0;i<n;i++){
  int flag=0;
  for(j=0;j<f;j++) if(fr[j]==p[i]){ flag=1; time[j]=++t; }
  if(!flag){ int rep=0; for(j=1;j<f;j++) if(time[j]<time[rep]) rep=j;
   fr[rep]=p[i]; time[rep]=++t; count++; }
  printf("%d: ",p[i]); for(k=0;k<f;k++) printf("%d ",fr[k]); printf("\n");
 }
 printf("Faults=%d\n",count);
}
