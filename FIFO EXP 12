#include <stdio.h>
int main(){
 int pages[20],frame[10],n,f,i,j=0,k,flag,count=0;
 printf("Enter no. of pages: "); scanf("%d",&n);
 printf("Enter page numbers: "); for(i=0;i<n;i++) scanf("%d",&pages[i]);
 printf("Enter frame size: "); scanf("%d",&f);
 for(i=0;i<f;i++) frame[i]=-1;
 printf("\nPage\tFrames\n");
 for(i=0;i<n;i++){
  flag=0;
  for(k=0;k<f;k++) if(frame[k]==pages[i]) flag=1;
  if(!flag){ frame[j]=pages[i]; j=(j+1)%f; count++; }
  printf("%d\t",pages[i]); for(k=0;k<f;k++) printf("%d ",frame[k]); printf("\n");
 }
 printf("Page Faults = %d\n",count);
}
